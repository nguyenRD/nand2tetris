/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    	Mux(a=in, b=false, sel=sel, out=a);
	Not(in=sel, out=sel1);
	Mux(a=in, b=false, sel=sel1, out=b);
}
